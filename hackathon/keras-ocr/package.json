{
  "name": "keras-ocr",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "--ship--": "------------------------------------------------------",
    "build:dev:device:ios": "eas build --profile development:device --platform ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:device:android": "eas build --profile development:device --platform android",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios  --non-interactive --no-wait",
    "build:preview:android": "eas build --profile preview --platform android  --non-interactive --no-wait",
    "build:ios": "eas build --profile production --platform ios  --non-interactive --no-wait",
    "build:android": "eas build --profile production --platform android  --non-interactive --no-wait",
    "update:dev": "eas update --branch development",
    "update:preview": "eas update --branch preview",
    "update": "eas update --branch production"
  },
  "dependencies": {
    "@infinitered/react-native-mlkit-image-labeling": "^0.5.7",
    "@shopify/restyle": "^2.4.2",
    "expo": "~49.0.15",
    "expo-dev-client": "~2.4.12",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.55.0",
    "eslint-config-react-native-wcandillon": "^3.10.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
